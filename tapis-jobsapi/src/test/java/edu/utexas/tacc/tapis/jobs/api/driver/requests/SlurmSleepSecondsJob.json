{
    "name": "SlurmSleepSeconds",
    "appId": "SlurmSleepSeconds",
    "appVersion": "0.0.1",
    "parameterSet": {
      "envVariables": [ 
        {"key": "JOBS_PARMS", "value": "15"}
      ],
      "archiveFilter": {
        "includes": ["Sleep*", "tapisjob.*"],
        "includeLaunchFiles": true
      }
    },
    "execSystemInputDir":  "HOST_EVAL($STRATCH)/jobs/${jobID}",
    "execSystemOutputDir": "HOST_EVAL($STRATCH)/jobs/${jobID}/output",
    "archiveSystemId" : "slurmStampede2",
    "archiveSystemDir": "HOST_EVAL($HOME)/jobs/${jobID}/archive"
}